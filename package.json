{
  "name": "logistic-dashboard-monitoring",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "format": "prettier --write \"src/**/*.{ts,html,css,json}\" --loglevel warn",
    "build-prettier": "npm run format",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:logistic-dashboard-monitoring": "node dist/logistic-dashboard-monitoring/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "bracketSameLine": true,
    "htmlWhitespaceSensitivity": "ignore",
    "plugins": [],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.7",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/three": "^0.182.0",
    "d3": "^7.9.0",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "lucide-angular": "^0.562.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "three": "^0.182.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.17.19",
    "@types/webworker": "^0.0.56",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2"
  }
}
